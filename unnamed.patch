+        if (proDialog == null) {
+
+            proDialog = MyDialogUtil.showRotateDialog(mContext, msg, cancelable);
+        } else {
+            if (!proDialog.isShowing()) {
+                proDialog = MyDialogUtil.showRotateDialog(mContext, msg, cancelable);
